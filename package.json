{
  "name": "@kamiya4047/eslint-plugin-pretty-import",
  "version": "0.1.2",
  "description": "ESLint plugin for sorting imports in a biased order",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamiya4047/eslint-plugin-pretty-import.git"
  },
  "bugs": {
    "url": "https://github.com/kamiya4047/eslint-plugin-pretty-import/issues"
  },
  "homepage": "https://github.com/kamiya4047/eslint-plugin-pretty-import#readme",
  "license": "GPL-3.0",
  "publishConfig": {
    "provenance": true
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "import",
    "sort",
    "organize"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "bun test test/",
    "test:watch": "bun test test/ --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "bun --watch src/index.ts",
    "docs": "echo 'Documentation available in docs/ folder'",
    "prepublishOnly": "bun run build && bun test",
    "publish:npm": "npm publish --registry https://registry.npmjs.org",
    "publish:github": "npm publish --registry https://npm.pkg.github.com",
    "publish:all": "npm run publish:npm && npm run publish:github",
    "release": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/bun": "latest",
    "@types/eslint": "^9",
    "@types/estree": "^1.0.8",
    "@typescript-eslint/parser": "^8.44.0",
    "@typescript-eslint/utils": "^8.44.0",
    "eslint": "^9",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "jiti": "^2.5.1",
    "rollup": "^4.24.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.44.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.44.0"
  }
}
